{
  "name": "indanis",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "cross-env BROWSER=none vite --port 3000",
    "build": "cross-env CI=false tsc && vite build",
    "npmrc": "tsx ./scripts/generate-npmrc",
    "dev": "tsx ./scripts/run-dev-environment",
    "dev:local": "npm-run-all --parallel docker:local start",
    "dev:remote": "npm-run-all --parallel docker:remote start",
    "test": "vitest run --silent",
    "test:verbose": "vitest run --reporter=verbose",
    "test:ui": "vitest --ui --coverage.enabled=true --coverage.reporters=html --coverage.directory=coverage/ui",
    "test:update-snapshots": "vitest run -u",
    "test:coverage": "vitest run --coverage --silent",
    "build-themes": "devextreme build",
    "lint": "npm run lint:eslint && npm run lint:styles",
    "lint:styles": "stylelint 'src/**/*.{scss,style.ts}'",
    "lint:eslint": "eslint src",
    "lint:eslint:fix": "eslint src --fix",
    "docker:local": "docker compose --profile full up",
    "docker:remote": "docker compose --profile frontend up",
    "build:api": "tsx ./scripts/build-api",
    "prepare": "husky install",
    "dictionary:build": "tsx ./scripts/dictionary/parse-dictionary",
    "dictionary:remove": "tsx ./scripts/dictionary/remove-dictionary",
    "dictionary:unused": "i18n-unused display-unused",
    "dictionary:mark": "i18n-unused mark-unused",
    "dictionary:missed": "i18n-unused display-missed"
  },
  "dependencies": {
